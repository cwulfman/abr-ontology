@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:  <http://www.w3.org/2002/07/owl#> .
@prefix xsd:  <http://www.w3.org/2001/XMLSchema#> .
@prefix oset: <http://oset.org/ontologies/abr/> .

#################################################################
# State class and properties
#################################################################

oset:State a owl:Class ;
    rdfs:label "State" .

oset:postalCode a owl:DatatypeProperty ;
    rdfs:domain oset:State ;
    rdfs:range  xsd:string ;
    rdfs:label  "postal code" ;
    rdfs:comment "The two-letter USPS postal code for a U.S. state." .

oset:hasState a owl:ObjectProperty ;
    rdfs:domain oset:ABRForm ;
    rdfs:range  oset:State ;
    rdfs:label  "has state" ;
    rdfs:comment "Links an absentee ballot request form type to the state that issues it." ;
    owl:inverseOf oset:hasABRForm .

oset:hasABRForm a owl:ObjectProperty ;
    rdfs:domain oset:State ;
    rdfs:range  oset:ABRForm ;
    rdfs:label  "has ABR form" ;
    rdfs:comment "Links a state to its absentee ballot request form type." .

